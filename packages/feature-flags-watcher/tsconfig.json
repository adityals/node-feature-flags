{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [
          "es5"
        ],
        "outDir": "./dist",
        "declarationDir": "./dist",
        "declaration": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "emitDeclarationOnly": true,
    },
    "exclude": ["__test__", "lib", "node_modules"],
    "include": ["./src"],
}